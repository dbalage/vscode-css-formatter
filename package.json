{
    "name": "vscode-stylesheet-formatter",
    "displayName": "Stylesheet Formatter",
    "description": "Formatter for CSS, SASS, LESS",
    "version": "0.2.0",
    "publisher": "dbalage",
    "icon": "images/logo.png",
    "galleryBanner": {
        "color": "#eff1f3",
        "theme": "light"
    },
    "categories": [
        "Languages",
        "Other"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/dbalage/vscode-css-formatter/issues"
    },
    "homepage": "https://github.com/dbalage/vscode-css-formatter/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/dbalage/vscode-css-formatter"
    },
    "engines": {
        "vscode": "^0.10.1"
    },
    "activationEvents": [
        "onLanguage:css",
        "onLanguage:sass",
        "onLanguage:scss",
        "onLanguage:less"
    ],
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
    },
    "dependencies": {
        "js-beautify": "^1.6.2"
    },
    "devDependencies": {
        "typescript": "*",
        "vscode": "0.11.x"
    }
}
